{
  "$schema": "./search-space-schema.json#",
  "name": "image_classification_option",
  "search_space": [
    {
      "name": "LabelColumnName",
      "type": "string",
      "default": "Label"
    },
    {
      "name": "ScoreColumnName",
      "type": "string",
      "default": "Score"
    },
    {
      "name": "FeatureColumnName",
      "type": "string",
      "default": "Feature"
    },
    {
      "name": "Arch",
      "type": "imageClassificationArchType",
      "default": "ResnetV250"
    },
    {
      "name": "BatchSize",
      "type": "integer",
      "default": 10
    },
    {
      "name": "Epoch",
      "type": "integer",
      "default": 200
    }
  ]
}
