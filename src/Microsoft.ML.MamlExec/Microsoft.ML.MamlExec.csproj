	<Compile Include="..\Microsoft.ML.Maml\ChainCommand.cs">
      <Link>ChainCommand.cs</Link>
    </Compile>
	<Compile Include="..\Microsoft.ML.Maml\HelpCommand.cs">
      <Link>HelpCommand.cs</Link>
    </Compile>
	<Compile Include="..\Microsoft.ML.Maml\VersionCommand.cs">
      <Link>VersionCommand.cs</Link>
    </Compile>
  </ItemGroup>

</Project>